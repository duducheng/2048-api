# Makefile.in generated by hand.
# @configure_input@

CC = @CC@
CFLAGS = @CFLAGS@
CPP = @CPP@
CPPFLAGS = @CPPFLAGS@
CXX = @CXX@
CXXLD = $(CXX)
CXXCPP = @CXXCPP@
CXXFLAGS = @CXXFLAGS@ -O3 -Wall -Wextra -fPIC
LDFLAGS = @LDFLAGS@
LIBS = @LIBS@
MKDIR_P = @MKDIR_P@

EXEEXT = @EXEEXT@
OBJEXT = @OBJEXT@

$(shell $(MKDIR_P) bin)

all: bin/2048$(EXEEXT) bin/2048.so

bin/%$(EXEEXT): bin/%.$(OBJEXT)
	$(CXXLD) $(CXXFLAGS) $(LDFLAGS) $^ $(LDLIBS) -o $@

bin/%.so: bin/%.$(OBJEXT)
	$(CXXLD) $(CXXFLAGS) -shared $(LDFLAGS) $^ $(LDLIBS) -o $@

bin/%.$(OBJEXT) : %.cpp
	$(CXX) $(CPPFLAGS) $(CXXFLAGS) -c -o $@ $<

clean:
	$(RM) -rf bin/*

.PHONY: all clean
